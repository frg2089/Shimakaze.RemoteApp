{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "StaticResourcesPath": "wwwroot",
  "DefaultIconPath": "C:\\Windows\\system32\\shell32.dll",
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": ".\\Shimakaze.RemoteApp.Web.pfx",
        "Password": "<Password>"
      }
    },
    "EndpointDefaults": {
      "ClientCertificateMode": "AllowCertificate",
      "SslProtocols": [ "Tls13", "Tls12" ],
      "Protocols": "Http1AndHttp2AndHttp3"
    }
  }
}
